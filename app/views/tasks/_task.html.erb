<div id="<%= dom_id task %>">
  <p>
    <strong>Name:</strong>
    <%= task.name %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= task.date %>
  </p>

  <p>
    <strong>Complete:</strong>
    <%= task.complete %>
  </p>

  <p>
    <strong>Overdue:</strong>
    <%= task.overdue %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= task.status %>
  </p>

</div>
